{
  "types": [
    "node",
    "jest",
    "@testing-library/jest-dom"
  ],
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "src/shared/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "src/*": [
        "src/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
